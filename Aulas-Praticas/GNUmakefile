OS = $(shell uname -s)

CC = gcc
AR = ar

ifeq ($(cc), CLANG)
CC = clang
endif

LD = $(CC)

DIALETO = ansi
STANDARD = -ansi

ifeq ($(dialeto), C89)
DIALETO = c89
STANDARD = -std=c89
endif

ifeq ($(dialeto), C90)
DIALETO = ansi
STANDARD = -std=c90
endif

ifeq ($(dialeto), C99)
DIALETO = c99
STANDARD = -std=c99
endif

ifeq ($(dialeto), C11)
DIALETO = c11
STANDARD = -std=c11
endif

ifeq ($(dialeto), C17)
DIALETO = c17
STANDARD = -std=c17
endif

CFLAGS = -Wall $(STANDARD)

LFLAGS = -Wall

