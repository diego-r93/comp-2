		$(LIBMATEMATICADOWHILE)\
		$(LIBMATEMATICAFOR)\
		$(LIBMATEMATICAWHILE)\
		$(LIBMATEMATICA)\
		$(LIBMONITOR)\
		$(LIBBASE)\
		$(LIBARQUIVOS)

ALL = $(EXECS) $(LIBS)

# Regra Implicita
.c.o:
	$(CC) $(CFLAGS) -c $<

all: $(ALL)

libmatematicarecursao.a: $(LIBMATEMATICARECURSAOOBJS)
	$(AR) -r -c $@ $^

